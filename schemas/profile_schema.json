{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["$schema", "kind", "name"],
  "properties": {
    "$schema": {
      "const": "profile_schema.json#"
    },
    "kind": {
      "const": "profile"
    },
    "name": {
      "description": "Name (database key) of this Profile",
      "type": "string"
    }
  }
}
